{
  "name": "litebook",
  "version": "0.0.0",
  "description": "Lite alternative to StoryBook using Vite",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --module esnext --noEmit",
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:next": "pnpm build && npm publish --tag next",
    "pub:release": "pnpm build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mnik01/litebook"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "litebook": "./dist/index.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "keywords": [
    "vite",
    "storybook",
    "typescript",
    "litebook"
  ],
  "author": "mnik01",
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "tsup": "^6.5.0"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "fs-extra": "^11.1.0"
  }
}
